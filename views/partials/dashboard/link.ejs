<% 
    let target = settings.LinkTarget ?? '_self';
%>

<a class="db-item db-link <%= model.Size || 'medium' %>" href="<%= model.Url%>" onclick="abortRequests()" target="<%= target %>">
    <div class="icon">
       <img src="<%= model.Icon %>" />
    </div>
    <div class="content">
        <div class="title"><%= model.Name %></div>
    </div>
    <div class="arrow"></div>
</a>